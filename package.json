{
  "name": "@akuskis/components-library",
  "version": "0.1.5",
  "private": false,
  "main": "build/index.js",
  "module": "build/index.js",
  "files": [
    "build",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/akuskis/components-library"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production babel src/components --out-dir build --copy-files --ignore test.js,__snapshots__",
    "test": "react-scripts test --env=jsdom",
    "eslint": "eslint .",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9000 -c .storybook -s ./public",
    "deploy-storybook": "storybook-to-ghpages -s ./public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && eslint . && CI=true npm test"
    }
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "peerDependencies": {
    "@material-ui/core": "3.x",
    "react": "16.x",
    "react-dom": "16.x",
    "react-router-dom": "4.x"
  },
  "devDependencies": {
    "@material-ui/core": "^3.1.2",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.24.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.82.0",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.7.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-test-renderer": "^16.5.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.16",
    "storybook-react-router": "^1.0.1"
  }
}
